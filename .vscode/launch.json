{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug cpp-data-structure Project",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/out/build/Clang 16.0.0 x86_64-apple-darwin24.0.0/bin/${input:executableName}",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "lldb",
            "preLaunchTask": "CMake Build",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerPath": "/usr/bin/lldb"
        }
    ],
    "inputs": [
        {
            "id": "executableName",
            "type": "promptString",
            "description": "Enter the executable name (e.g., Section00/00-02-mainBaic)"
        }
    ]
}